{
    "name": "WhitGGCalendar",
    "version": "1.1",
    "description": "This extension will create a calendar on your google calendar base on your class's schedule",
    "manifest_version": 2,
    "content_scripts": [
      {
        "matches": ["https://my.whitman.edu/mobile"],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_title": "WhitGGCalendar"
    },
    "key" : "",
    "oauth2": {
      "client_id": "818473115331-6hm7tpd725lpmjhr8m45fo9luu7i6ink.apps.googleusercontent.com",
      "scopes": ["https://www.googleapis.com/auth/calendar"]
    },
    "background": {
      "script": ["background.js"],
      "page": "background.html",
      "persistent": false
    },
    "permissions": ["identity", "activeTab"],
    "content_security_policy": "script-src 'self' https://apis.google.com/; object-src 'self'"
}
